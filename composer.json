{
    "name": "dcat/laravel-admin",
    "description": "dcat admin",
    "type": "library",
    "keywords": ["dcat", "laravel", "admin", "grid", "form", "laravel admin"],
    "homepage": "https://github.com/jqhph/dcat-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "jqh",
            "email": "841324345@qq.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "laravel/framework": "~12.0|~11.0",
        "laravel/sanctum": "^3|^4",
        "spatie/eloquent-sortable": "3.*|4.*",
        "doctrine/dbal": "^3.0|^4"
    },
    "require-dev": {
        "laravel/dusk": "~7.0|~8.0",
        "phpstan/phpstan": "^0.12.0|~1.0",
        "phpunit/phpunit": "~9|~10|~11",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Dcat\\Admin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dcat\\Admin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcat\\Admin\\AdminServiceProvider"
            ]
        }
    }
}
